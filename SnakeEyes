<p id="p1"></p>
<p id= "p2"></p>
<p id= "p3"></p>
<script>
	var dice1 = Math.floor((Math.random()*6)+1);
	var dice2 = Math.floor((Math.random()*6)+1);

	document.getElementById("p1").innerHTML= "First die: " + dice1;

	document.getElementById("p2").innerHTML= "Second die: " + dice2;


	if ((dice1== 1) && (dice2==1)){
	document.getElementById("p3").innerHTML= 			"Snake eyes";
	}
	else{
	document.getElementById("p3").innerHTML= 			"Not snake eyes";

	}

		</script>
	</body>
